<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:background="#FFFFFF"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/notosansbold"
            android:gravity="center"
            android:text="당신은 트렌드에 엄격한"
            android:textSize="18sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="X 세대"
            android:textColor="#FFC100"
            android:textSize="23sp"
            android:gravity="center_horizontal"
            android:layout_marginTop="-30dp"
            android:fontFamily="@font/notosansbold"/>
        <ImageView
            android:layout_width="200dp"
            android:layout_height="320dp"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/x"
            android:layout_marginTop="-20dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F6FE98"
            android:text="각자의 개성을 드러내기 시작한 세대다."
            android:textSize="16sp"
            android:gravity="center_horizontal"
            android:fontFamily="@font/notosansthink"
            android:textColor="#000000"
            android:layout_marginTop="-15dp" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="✔ X 세대의"
            android:layout_marginLeft="125dp"
            android:textColor="#000000"
            android:textSize="19sp"
            android:fontFamily="@font/notosansregular"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="유행어"
            android:fontFamily="@font/notosansregular"
            android:textSize="20.5sp"
            android:layout_marginTop="-57sp"
            android:layout_marginLeft="230dp"
            android:textColor="#FFC501"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="조기"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="조기 퇴직"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="명태"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="명예 퇴직"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="황태"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="황당하게 퇴직"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="입당"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="당뇨 생김"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="니 똥 칼라똥이다"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="너 잘난 거 알겠으니까 좀 조용하라는 뜻"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="7700"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="뛰뛰빵빵, 드라이브 가자는 뜻"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="오렌지족"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="화려한 소비생활을 즐겼던 20대 청년"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Y2K"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="종말론"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="깔다구"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="남자친구"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="삐삐쳐"
            android:textSize="20sp"
            android:textColor="#000000"
            android:fontFamily="@font/notosansbold"
            android:layout_marginLeft="15dp"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="연락해"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-15dp"
            android:textSize="15sp"/>
    </LinearLayout>
</ScrollView>